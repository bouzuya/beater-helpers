{
  "name": "beater-helpers",
  "description": "A collection of helper functions for beater",
  "version": "0.2.0",
  "author": {
    "email": "m@bouzuya.net",
    "name": "bouzuya",
    "url": "http://bouzuya.net/"
  },
  "babel": {
    "presets": [
      "power-assert"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/beater-helpers/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@types/node": "^12.0.0",
    "@types/power-assert": "^1.5.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.11",
    "babel-preset-power-assert": "^3.0.0",
    "beater": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "proxyquire": "^2.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "watch": "^1.0.2"
  },
  "files": [
    "fixture.d.ts",
    "fixture.js",
    "index.d.ts",
    "index.js"
  ],
  "homepage": "https://github.com/bouzuya/beater-helpers#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bouzuya/beater-helpers.git"
  },
  "scripts": {
    "build": "npm-run-all -s \"build:lint\" \"build:es2015\" \"build:es5\" \"build:lib\"",
    "build:es2015": "tsc",
    "build:es5": "babel --out-dir .tmp/es5/ .tmp/es2015/",
    "build:lib": "babel --copy-files --no-comments --out-dir . .tmp/es2015/src/",
    "build:lint": "tslint --project tsconfig.json",
    "clean": "rimraf \".tmp\" \"*.js\" \"*.d.ts\"",
    "prepare": "npm-run-all -s \"clean\" \"build\"",
    "test": "node .tmp/es5/test/index.js",
    "watch": "npm-run-all -p \"watch:*\"",
    "watch:es2015": "npm run build:es2015 -- --watch",
    "watch:es5": "npm run build:es5 -- --watch",
    "watch:test": "watch \"npm run test\" \".tmp/es5/\""
  },
  "types": "index.d.ts"
}
